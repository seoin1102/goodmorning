<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="crew">	
		<select id="findAll" parameterType="long" resultType="crewvo">
				<![CDATA[
				select no, channel_no as channelNo, master_crew_user_no as masterCrewUserNo, creation_date as creationDate, name
				from crew where channel_no = #{channelNo }
				]]>
		</select>
		<insert id="insert" parameterType="crewvo">
				<![CDATA[
				insert
					into channel
				values(null, #{channelNo }, #{masterCrewUserNo }, now(), #{name })
				]]>
		</insert>
</mapper>
